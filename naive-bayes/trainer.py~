


import simplejson



def process_raw_data(rawData):
    return rawData.map(simplejson.loads)



def train(rawData, categorize):
    
    #categorizedReviews = a list of (C, T) pairs where C = category and T = review text
    categorizedReviews = process_raw_data(rawData).map(lambda x: (categorize(x), x["text"])).cache()
    
    
